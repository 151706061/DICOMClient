<DicomClientConfig>

    
    <!-- URL for the service. -->
    <DicomServiceUrl>https://141.214.124.202:8091</DicomServiceUrl>       <!-- rolxdeploy1 -->
    <!-- <DicomServiceUrl>https://141.214.124.240:8091</DicomServiceUrl> -->   <!-- rolxdev1 -->
    <!-- <DicomServiceUrl>https://141.214.125.70:8091</DicomServiceUrl> -->    <!-- irrer desktop -->
    
    <!-- Used to generate the default patient id.  Special character meanings:
        * : a random (capital) alpha or digit : ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789
        ? : random capital letter : ABCDEFGHIJKLMNOPQRSTUVWXYZ
        # : a random digit : 0123456789
        % : escape a special character
        
        Other characters are taken literally.  Examples:
        
          input         could generate
          - - - - - - - - - - - - - - -
          ANON??##  ==> ANON3J42
          %?FOO%%   ==> ?FOO%  (no randomness)
          X######   ==> X087853
        
        -->
    <AnonPatientIdTemplate>$######</AnonPatientIdTemplate>
    
    <!-- When generating UIDs for anonymization, this prefix is used.   If not specified, a default is used.
    Note that a value of 1.3.6.1.4.1.22361. is for University of Michigan Department of Radiation Oncology. -->
    <RootGuid>9999.1.3.6.1.4.1.22361.</RootGuid>
    
    <!-- If true, upload help is shown.  It makes sense to not show the upload help for sites where the server is not installed. -->
    <ShowUploadHelp>true</ShowUploadHelp>
    
    <!-- Location of java keystore file.  If this value is specified by the
    system property javax.net.ssl.trustStore then the system property will
    override these entries.  Multiple entries indicate that
    multiple locations should be tried in the given order. -->
    <javax.net.ssl.trustStore>dicomsvc.jks</javax.net.ssl.trustStore>
    <javax.net.ssl.trustStore>src/main/resources/dicomsvc.jks</javax.net.ssl.trustStore>
    
   
